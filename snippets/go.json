{
	"if err != nil": {
		"prefix": "iferr",
		"body": [
			"if err != nil {",
			"\t$1",
			"}"
		],
		"description": "if err != nil"
	},
	"defer statement": {
		"prefix": "def",
		"body": [
			"defer func(){ $1 }()"
		],
		"description": "Snippet for defer statement."
	},
	"fmt.Printf": {
		"prefix": "fmtf",
		"body": [
			"fmt.Printf(\"%#v\\n\", $1)"
		],
		"description": "Snippet for fmt.Printf()."
	},
	"fmt.Println": {
		"prefix": "fmtp",
		"body": [
			"fmt.Println($1)"
		],
		"description": "Snippet for fmt.Println()."
	},
	"log.Println": {
		"prefix": "logp",
		"body": [
			"log.Println($1)"
		],
		"description": "Snippet for fmt.Println()."
	},
	"fmt.Errorf": {
		"prefix": "loge",
		"body": [
			"log.Errorf(\"%#v\\n\", $1)"
		],
		"description": "Snippet for fmt.Errorf()."
	},
	"log.Printf": {
		"prefix": "logf",
		"body": [
			"log.SetFlags(log.LstdFlags | log.Llongfile)",
			"log.Printf(\"%#v\\n\", $1)"
		],
		"description": "Snippet for log.Printf() with option."
	},
	"type struct declaration": {
		"prefix": "struct",
		"body": [
			"type ${1: name} struct {",
			"$2",
			"}"
		],
		"description": "Snippet for struct declaration."
	},
	"type interface declaration": {
		"prefix": "interface",
		"body": [
			"type ${1:name} interface { ",
			"$2",
			"}"
		],
		"description": "Snippet for interface declaration."
	},
	"make(...)": {
		"prefix": "make",
		"body": "make(${1:type}, ${2:0})",
		"description": "Snippet for make statement"
	},
	"method declaration": {
		"prefix": "method",
		"body": [
			"func (${1:receiver}) ${2:method}(${3:type}) $4 {",
			"$6",
			"}"
		],
		"description": "Snippet for method declaration."
	},
	"benchmark function": {
		"prefix": "bf",
		"body": "func Benchmark$1(b *testing.B) {\n\tfor ${2:i} := 0; ${2:i} < b.N; ${2:i}++ {\n\t\t$0\n\t}\n}",
		"description": "Snippet for Benchmark function."
	},
	"requestBodyPrint": {
		"prefix": "rcheck",
		"body": [
			"buf := make([]byte, 2048)",
			"n, _ := c.Request.Body.Read(buf)",
			"b := string(buf[0:n])",
			"log.Printf(\"%#v\\n\", $1)"
		],
		"description": "Snippet for request body check.(with gin context)"
	},
	"init mock": {
		"prefix": "newMock",
		"body": [
			"ctrl := gomock.NewController(t)",
			"mock := ${1:init method}(ctrl)",
			"tt.prepareMockFn(mock)"
		],
		"description": "Snippet for init mock."
	}
}
